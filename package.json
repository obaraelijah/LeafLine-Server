{
  "name": "leafline",
  "version": "3.23.10",
  "description": "The LeafLine Book Store is an online platform that aims to provide a user-friendly and efficient way for customers to explore, purchase, and manage books.",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "dev": "nodemon backend/server.js",
    "start": "node backend/server.js",
    "coverage": "nyc --reporter=text-lcov jest && nyc report --reporter=text-summary"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "verbose": true,
    "detectOpenHandles": true,
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "author": "Elijah Samson <elijahobara357@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.37.0",
		"chalk": "^5.1.2",
		"cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
		"express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-brute": "^1.0.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3"
  }
}